{
  "name": "google-pub-sub-authorizer",
  "projectName": "support::google-pub-sub-authorizer",
  "description": "Authorizor for Google Pub Sub API. Currently used by Subscribe with Google",
  "repository": "https://github.com/guardian/subscribe-with-google",
  "version": "1.0.0",
  "private": true,
  "cloudformation": "google-pub-sub-authorizer-lambda/cloudformation.yaml",
  "riffraffFile": "google-pub-sub-authorizer-lambda/riff-raff.yaml",
  "isAwsLambda": true,
  "uploadArtefact": true,
  "buildDir": "google-pub-sub-authorizer-lambda",
  "scripts": {
    "deploy-lambda": "VERBOSE=true ARTEFACT_PATH=$PWD riffraff-artefact"
  },
  "devDependencies": {
    "node-riffraff-artefact": "^2.0.1"
  }
}
